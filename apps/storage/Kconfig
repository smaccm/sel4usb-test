    bool "USB STORAGE"
    default y
    depends on HAVE_LIBC
    depends on LIB_SEL4_PLAT_SUPPORT
    depends on LIB_SEL4_VKA
    depends on LIB_SEL4_UTILS
    depends on LIB_UTILS
    depends on LIB_CPIO
    depends on LIB_SEL4
    depends on LIB_ELF
    depends on LIB_USB
    depends on LIB_SEL4_DMA
    help
        USB storage app.
